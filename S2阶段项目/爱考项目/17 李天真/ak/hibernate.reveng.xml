<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="t_student" schema="dbo" catalog="db_exam"
		class="com.qhit.hl.grl.ltz.common.bean.Student">
		<primary-key>
			<generator class="foreign"></generator>
		</primary-key>
	</table>
	<table name="t_teacher" schema="dbo" catalog="db_exam"
		class="com.qhit.hl.grl.ltz.common.bean.Teacher">
		<primary-key>
			<generator class="foreign"></generator>
		</primary-key>
	</table>
	<table name="t_user" schema="dbo" catalog="db_exam"
		class="com.qhit.hl.grl.ltz.common.bean.User">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_classInfo" schema="dbo" catalog="db_exam"
		class="com.qhit.hl.grl.ltz.bjgl.bean.ClassInfo">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_course" schema="dbo" catalog="db_exam"
		class="com.qhit.hl.grl.ltz.kmgl.bean.Course">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_major" schema="dbo" catalog="db_exam"
		class="com.qhit.hl.grl.ltz.zysz.bean.Major">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_question" schema="dbo" catalog="db_exam"
		class="com.qhit.hl.grl.ltz.tkgl.bean.Question">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
	<table name="t_machineTest" schema="dbo" catalog="db_exam"
		class="com.qhit.hl.grl.ltz.tkgl.bean.MachineTest">
		<primary-key>
			<generator class="native"></generator>
		</primary-key>
	</table>
<table name="t_writtenTest" schema="dbo" catalog="db_exam"
	class="com.qhit.hl.grl.ltz.tkgl.bean.WrittenTest">
	<primary-key>
		<generator class="native"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>